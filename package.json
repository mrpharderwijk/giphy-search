{
  "name": "giphy-search",
  "version": "0.0.1",
  "scripts": {
    "build": "ng build --aot",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage",
    "e2e": "ng e2e",
    "ng": "ng",
    "postinstall": "ng build --aot --prod",
    "pretty": "prettier --parser typescript --loglevel warn --write \"src/**/*.ts\"",
    "serve:local": "ng serve --aot --port 4500 --configuration=local --open",
    "serve:prod": "ng serve --aot --port 4500 --configuration=production",
    "start": "node server.js",
    "test": "ng test --code-coverage --watch=true"
  },
  "precommit": [
    "pretty",
    "lint",
    "postinstall"
  ],
  "engines": {
    "node": "11.0.0",
    "npm": "6.8.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@types/giphy-api": "^2.0.2",
    "badwords-list": "^1.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "http-server": "^0.12.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-scss-preprocessor": "^4.0.0",
    "post-commit": "0.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "1.16.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-defocus": "^2.0.6",
    "typescript": "~3.8.3"
  }
}
